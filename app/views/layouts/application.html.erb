<!DOCTYPE html>
<html>
  <head>
    <title>OauthPractice</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if @current_user %>
  <nav>
    <%= link_to 'Log out', log_out_path %>
  </nav>
  <% end %>
    <%= yield %>
  </body>
</html>
